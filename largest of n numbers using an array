
#include <stdio.h>

int main()
{
    int a[100], size, max, i;
    printf("Enter size of an array: \n");
    scanf("%d", &size);
    printf("Enter value in array: \n");
    for(i = 0; i < size; i++)
    {
        scanf("%d", &a[i]);
    }
    max = a[0];
    for(i = 0; i < size; i++)
    {
        if(a[i]>max)
        {
            max = a[i];
        }
    }
    printf("Maximum value is %d", max);
    

    return 0;
}
